__lua__
-- game loop
	
function _init()

	light_state_n = "red"
	light_state_s = "green"
	light_state_e = "green"
	light_state_w = "red"
end

function _update()
 if time() > 10 then
 	_light_n(10,10/3)
		_light_w(10,10/3)
		_light_s(10/3,10)
		_light_e(10,10/3)
	else
		_light_n(10 - 10/3,10/3)
		_light_w(10- (10/3)*2,10/3)
		_light_s(10/3,10)
		_light_e(10,10/3)
	end

end

function _draw()
 cls()
 draw_map()
 
 
 -- northern traffic light
 if light_state_n == "red" then
  spr(1,76,32)
 elseif light_state_n == "green" then
 	spr(2,76,32)
 end
 
 -- southern traffic light
 if light_state_s == "red" then
  spr(1,52,88)
 elseif light_state_s == "green" then
 	spr(2,52,88)
 end
 
 -- eastern traffic light
 if light_state_e == "red" then
  spr(1,92,72)
 elseif light_state_e == "green" then
 	spr(2,92,72)
 end
 
 -- western traffic light
 if light_state_w == "red" then
  spr(1,36,48)
 elseif light_state_w == "green" then
 	spr(2,36,48)
 end
 
end
